{
    "name": "axyr/laravel-tractor",
    "description": "Opiniated Laravel CRUD generator for a Module/Repository/Filter based CRUD setup.",
    "type": "library",
    "keywords": ["laravel", "module", "crud", "api", "json", "boilerplate", "scaffolder"],
    "license": "MIT",
    "authors": [
        {
            "name": "Martijn van Nieuwenhoven",
            "email": "info@axyrmedia.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "Axyr\\Tractor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Axyr\\Tractor\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "wikimedia/composer-merge-plugin": "^2.1",
        "spatie/laravel-permission": "^6.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "orchestra/testbench": "^9.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Axyr\\Tractor\\TractorServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "wikimedia/composer-merge-plugin": true
        }
    }
}
