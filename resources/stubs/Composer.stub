{
    "name": "{{composerName}}",
    "description": "{{moduleName}}",
    "autoload": {
        "psr-4": {
            "{{autoloadPath}}\\": "{{srcDirectory}}/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "{{autoloadPath}}\\Tests\\": "{{testDirectory}}/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "{{serviceProviderFullyQyalifiedClassName}}"
            ]
        }
    }
}
